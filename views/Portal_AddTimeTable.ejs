<!doctype html>
<html lang="en">

<head>
    <%- include ('./UserPortal/vendar/Metahead.ejs') %>

</head>

<body>
    <!--  Body Wrapper -->
    <div class="page-wrapper" id="main-wrapper" data-layout="vertical" data-navbarbg="skin6" data-sidebartype="full"
        data-sidebar-position="fixed" data-header-position="fixed">
        <!-- Sidebar Start -->
        <%- include ('./UserPortal/vendar/sidebar.ejs') %>
            <!--  Sidebar End -->
            <!--  Main wrapper -->
            <div class="body-wrapper">
                <!--  Header Start -->
                <%- include ('./UserPortal/vendar/menubar.ejs') %>
                    <!--  Header End -->
                    <div class="container-fluid">
                        <!--  Row 1 -->
                        <div class="row">

                            <div class="card w-100">
                                <h5 class="card-title fw-semibold m-4">Add TimeTable</h5>
                                <div class="card-body p-3">
                                    <div class="table-responsive">
                                        <table class="table text-nowrap mb-0 align-middle">
                                            <thead class="text-dark fs-4">
                                                <tr>
                                                    <th class="border-bottom-0">
                                                        <h6 class="fw-semibold mb-0">No</h6>
                                                    </th>
                                                    <th class="border-bottom-0">
                                                        <h6 class="fw-semibold mb-0">Image</h6>
                                                    </th>
                                                    <th class="border-bottom-0">
                                                        <h6 class="fw-semibold mb-0">Er No</h6>
                                                    </th>
                                                    <th class="border-bottom-0">
                                                        <h6 class="fw-semibold mb-0">Name</h6>
                                                    </th>
                                                    
                                                    <th class="border-bottom-0">
                                                        <h6 class="fw-semibold mb-0">Add TimeTable</h6>
                                                    </th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <% var i=1 %>
                                                    <% allFaculty.forEach((item)=>{ %>
                                                        <tr>
                                                            <td class="border-bottom-0">
                                                                <h6 class="fw-semibold mb-0">
                                                                    <%=i++%>
                                                                </h6>
                                                            </td>
                                                            <td>
                                                                <img src="../upload/<%=item.Faculty_Image%>" alt=""
                                                                    width="35" height="35" class="rounded-circle">
                                                            </td>
                                                            <td class="border-bottom-0">
                                                                <p class="mb-0 fw-normal">
                                                                    <%=item.Faculty_ID%>
                                                                </p>
                                                            </td>
                                                            <td class="border-bottom-0">
                                                                <p class="mb-0 fw-normal">
                                                                    <%=item.FirstName%>
                                                                </p>
                                                                <p class="mb-0 fw-normal">
                                                                    <%=item.LastName%>
                                                                </p>
                                                            </td>
                                                            
                                                            <form action="AddTimeTable" method="post"
                                                                enctype="multipart/form-data">
                                                                <td>
                                                                    <div class="mb-3">
                                                                        <label for="exampleInputEmail1"
                                                                            class="form-label">TimeTable
                                                                        </label>
                                                                        <input type="file" name="TimeTable" required
                                                                            class="form-control">
                                                                        <input type="text" name="Faculty_ID" hidden
                                                                            value="<%=item.Faculty_ID%>" required
                                                                            class="form-control">
                                                                    </div>
                                                                </td>
                                                                <td class="border-bottom-0">
                                                                    
                                                                            <button type="submit"
                                                                                class="btn btn-primary">Submit</button>
                                                                          
                                                                </td>


                                                            </form>

                                                        </tr>
                                                        <%})%>

                                            </tbody>
                                        </table>
                                    </div>
                                </div>

                            </div>

                            <div class="card w-100">
                                <h5 class="card-title fw-semibold m-4">Add TimeTable</h5>
                                <div class="card-body p-3">
                                    <div class="table-responsive">
                                        <table class="table text-nowrap mb-0 align-middle">
                                            <thead class="text-dark fs-4">
                                                <tr>
                                                    <th class="border-bottom-0">
                                                        <h6 class="fw-semibold mb-0">No</h6>
                                                    </th>
                                        
                                                    <th class="border-bottom-0">
                                                        <h6 class="fw-semibold mb-0">Year</h6>
                                                    </th>
                                                    
                                                    <th class="border-bottom-0">
                                                        <h6 class="fw-semibold mb-0">Add TimeTable</h6>
                                                    </th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                              
                                                        <tr>
                                                            <td class="border-bottom-0">
                                                                <h6 class="fw-semibold mb-0">
                                                                    1
                                                                </h6>
                                                            </td>
                                                            
                                                            <td class="border-bottom-0">
                                                                <p class="mb-0 fw-normal">
                                                                    I
                                                                </p>
                                                            </td>
                                                          
                                                            <form action="/AddTimeTable" method="post"
                                                                enctype="multipart/form-data">
                                                                <td>
                                                                    <div class="mb-3">
                                                                        <label for="exampleInputEmail1"
                                                                            class="form-label">TimeTable
                                                                        </label>
                                                                        <input type="file" name="TimeTable" required
                                                                            class="form-control">
                                                                        <input type="text" name="Faculty_ID" hidden
                                                                            value="1" required
                                                                            class="form-control">
                                                                    </div>
                                                                </td>
                                                                <td class="border-bottom-0">
                                                                    <% if (YearTimeTable.includes("1")){%>
                                                                        <button type="button"
                                                                            class="btn btn-success">Done</button>
                                                                        <%}else{%>
                                                                            <button type="submit"
                                                                                class="btn btn-primary">Submit</button>
                                                                            <%}%>
                                                                </td>


                                                            </form>

                                                        </tr>
                                                        <tr>
                                                            <td class="border-bottom-0">
                                                                <h6 class="fw-semibold mb-0">
                                                                    2
                                                                </h6>
                                                            </td>
                                                            
                                                            <td class="border-bottom-0">
                                                                <p class="mb-0 fw-normal">
                                                                    II
                                                                </p>
                                                            </td>
                                                          
                                                            <form action="/AddTimeTable" method="post"
                                                                enctype="multipart/form-data">
                                                                <td>
                                                                    <div class="mb-3">
                                                                        <label for="exampleInputEmail1"
                                                                            class="form-label">TimeTable
                                                                        </label>
                                                                        <input type="file" name="TimeTable" required
                                                                            class="form-control">
                                                                        <input type="text" name="Faculty_ID" hidden
                                                                            value="2" required
                                                                            class="form-control">
                                                                    </div>
                                                                </td>
                                                                <td class="border-bottom-0">
                                                                    <% if (YearTimeTable.includes("2")){%>
                                                                        <button type="button"
                                                                            class="btn btn-success">Done</button>
                                                                        <%}else{%>
                                                                            <button type="submit"
                                                                                class="btn btn-primary">Submit</button>
                                                                            <%}%>
                                                                </td>


                                                            </form>

                                                        </tr>
                                                      
                                                        <tr>
                                                            <td class="border-bottom-0">
                                                                <h6 class="fw-semibold mb-0">
                                                                    3
                                                                </h6>
                                                            </td>
                                                            
                                                            <td class="border-bottom-0">
                                                                <p class="mb-0 fw-normal">
                                                                    III
                                                                </p>
                                                            </td>
                                                          
                                                            <form action="AddTimeTable" method="post"
                                                                enctype="multipart/form-data">
                                                                <td>
                                                                    <div class="mb-3">
                                                                        <label for="exampleInputEmail1"
                                                                            class="form-label">TimeTable
                                                                        </label>
                                                                        <input type="file" name="TimeTable" required
                                                                            class="form-control">
                                                                        <input type="text" name="Faculty_ID" hidden
                                                                            value="3" required
                                                                            class="form-control">
                                                                    </div>
                                                                </td>
                                                                <td class="border-bottom-0">
                                                                    <% if (YearTimeTable.includes("3")){%>
                                                                        <button type="button"
                                                                            class="btn btn-success">Done</button>
                                                                        <%}else{%>
                                                                            <button type="submit"
                                                                                class="btn btn-primary">Submit</button>
                                                                            <%}%>
                                                                </td>


                                                            </form>

                                                        </tr>
                                                      
                                                        <tr>
                                                            <td class="border-bottom-0">
                                                                <h6 class="fw-semibold mb-0">
                                                                    4
                                                                </h6>
                                                            </td>
                                                            
                                                            <td class="border-bottom-0">
                                                                <p class="mb-0 fw-normal">
                                                                    IV
                                                                </p>
                                                            </td>
                                                          
                                                            <form action="AddTimeTable" method="post"
                                                                enctype="multipart/form-data">
                                                                <td>
                                                                    <div class="mb-3">
                                                                        <label for="exampleInputEmail1"
                                                                            class="form-label">TimeTable
                                                                        </label>
                                                                        <input type="file" name="TimeTable" required
                                                                            class="form-control">
                                                                        <input type="text" name="Faculty_ID" hidden
                                                                            value="3" required
                                                                            class="form-control">
                                                                    </div>
                                                                </td>
                                                                <td class="border-bottom-0">
                                                                    <% if (YearTimeTable.includes("3")){%>
                                                                        <button type="button"
                                                                            class="btn btn-success">Done</button>
                                                                        <%}else{%>
                                                                            <button type="submit"
                                                                                class="btn btn-primary">Submit</button>
                                                                            <%}%>
                                                                </td>


                                                            </form>

                                                        </tr>
                                                      
                                                      

                                            </tbody>
                                        </table>
                                    </div>
                                </div>

                            </div>

                        </div>

                    </div>
                    <%- include ('./UserPortal/vendar/footer.ejs') %>

            </div>
    </div>
    <%- include ('./UserPortal/vendar/scripts.ejs') %>
</body>

</html>