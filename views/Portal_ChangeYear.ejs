<!doctype html>
<html lang="en">

<head>
    <%- include ('./UserPortal/vendar/Metahead.ejs') %>

</head>

<body>
    <!--  Body Wrapper -->
    <div class="page-wrapper" id="main-wrapper" data-layout="vertical" data-navbarbg="skin6" data-sidebartype="full"
        data-sidebar-position="fixed" data-header-position="fixed">
        <!-- Sidebar Start -->
        <%- include ('./UserPortal/vendar/sidebar.ejs') %>
            <!--  Sidebar End -->
            <!--  Main wrapper -->
            <div class="body-wrapper">
                <!--  Header Start -->
                <%- include ('./UserPortal/vendar/menubar.ejs') %>
                    <!--  Header End -->
                    <div class="container-fluid">
                        <!--  Row 1 -->
                        <div class="row">

                            <div class="card w-100">
                                <h5 class="card-title fw-semibold m-4">Add TimeTable</h5>
                                <div class="card-body p-3">
                                    <div class="table-responsive">
                                        <table class="table text-nowrap mb-0 align-middle">
                                            <thead class="text-dark fs-4">
                                                <tr>
                                                    <th class="border-bottom-0">
                                                        <h6 class="fw-semibold mb-0">Transfer</h6>
                                                    </th>
                                                    <th class="border-bottom-0">
                                                        <h6 class="fw-semibold mb-0">Make it</h6>
                                                    </th>

                                                </tr>
                                            </thead>
                                            <tbody>

                                                <tr>
                                                    <td class="border-bottom-0">
                                                        <h6 class="fw-semibold mb-0">
                                                            <h6 class="fw-semibold mb-0">Transfer Student I to II</h6>
                                                        </h6>
                                                    </td>
                                                    <td class="border-bottom-0">
                                                        <h6 class="fw-semibold mb-0">
                                                            <form action="/changeyear" method="post">
                                                                <input type="text" name="year" value="1" hidden>
                                                            <button type="submit"
                                                                class="btn btn-primary">Change</button>
                                                            </form>
                                                        </h6>
                                                    </td>

                                                </tr>
                                                <tr>
                                                    <td class="border-bottom-0">
                                                        <h6 class="fw-semibold mb-0">
                                                            <h6 class="fw-semibold mb-0">Transfer Student II to III</h6>
                                                        </h6>
                                                    </td>
                                                    <td class="border-bottom-0">
                                                        <h6 class="fw-semibold mb-0">
                                                            <form action="/changeyear" method="post">
                                                                <input type="text" name="year" value="2" hidden>
                                                            <button type="submit"
                                                                class="btn btn-primary">Change</button>
                                                            </form>
                                                        </h6>
                                                    </td>

                                                </tr>
                                                <tr>
                                                    <td class="border-bottom-0">
                                                        <h6 class="fw-semibold mb-0">
                                                            <h6 class="fw-semibold mb-0">Transfer Student III to IV</h6>
                                                        </h6>
                                                    </td>
                                                    <td class="border-bottom-0">
                                                        <h6 class="fw-semibold mb-0">
                                                            <form action="/changeyear" method="post">
                                                                <input type="text" name="year" value="3" hidden>
                                                            <button type="submit"
                                                                class="btn btn-primary">Change</button>
                                                            </form>
                                                        </h6>
                                                    </td>

                                                </tr>
                                                <tr>
                                                    <td class="border-bottom-0">
                                                        <h6 class="fw-semibold mb-0">
                                                            <h6 class="fw-semibold mb-0">Transfer Student IV to Passout</h6>
                                                        </h6>
                                                    </td>
                                                    <td class="border-bottom-0">
                                                        <h6 class="fw-semibold mb-0">
                                                            <form action="/changeyear" method="post">
                                                                <input type="text" name="year" value="4" hidden>
                                                            <button type="submit"
                                                                class="btn btn-primary">Change</button>
                                                            </form>
                                                        </h6>
                                                    </td>

                                                </tr>


                                            </tbody>
                                        </table>
                                    </div>
                                </div>

                            </div>


                        </div>

                    </div>
            </div>
    </div>
    <%- include ('./UserPortal/vendar/scripts.ejs') %>
</body>

</html>